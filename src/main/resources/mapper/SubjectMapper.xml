<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nfsn.mapper.SubjectMapper">

    <resultMap id="BaseResultMap" type="com.nfsn.model.entity.Subject">
            <id property="subjectId" column="subject_id" jdbcType="INTEGER"/>
            <result property="subjectName" column="subject_name" jdbcType="VARCHAR"/>
            <result property="answer" column="answer" jdbcType="VARCHAR"/>
            <result property="subjectType" column="subject_type" jdbcType="TINYINT"/>
            <result property="subjectAnalysis" column="subject_analysis" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        subject_id,subject_name,answer,
        subject_type,subject_analysis
    </sql>
</mapper>
